/* template 6 */
.ks-dropbox-deepblue_dropbox .ks-dropbox-title {
 	font-size: 14px;
    font-weight: bold;
    line-height: 16px;
    margin-bottom: 5px;
    display: block;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-field {
    border: 1px solid #8d8d8d;
    border-radius: 5px;
    height: 40px;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-input {
    font-size: 14px;
    padding: 0 10px;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-input::-webkit-input-placeholder {
    color: #838388;
}
.ks-dropbox-deepblue_dropbox .ks-dropbox-input::-moz-placeholder {
    color: #838388;
}
.ks-dropbox-deepblue_dropbox .ks-dropbox-input:-ms-input-placeholder {
    color: #838388;
}
.ks-dropbox-deepblue_dropbox .ks-dropbox-input:-moz-placeholder {
    color: #838388;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-icon {
    border-radius: 4px;
    height: 40px;
    margin: 0 5px;
    width: 40px;
    display: block;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-icon:after {
    border-top: 5px solid #000;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    content: "";
    margin-top: -2.5px;
    position: absolute;
    right: 14px;
    top: 50%;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel {
    border: 1px solid #8d8d8d;
    border-radius: 5px;
    margin-top: 2px;
    padding: 2px;
    left: 8px;
    right: 8px;
    background-color: #fff;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-item {
    border-radius: 4px;
    height: 40px;
    padding: 0 10px;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-item:hover {
    background-color: #bfd9f2;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-item.on:hover {
    background-color: #0066cc;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-item.on {
    background-color: #0066cc;
    color: #fff;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-item.on .ks-dropbox-panel-item-text {
    color: #fff;
    font-size: 14px;
    font-weight: bold;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-item-text {
    color: #000;
    font-size: 14px;
    font-weight: bold;
}

.ks-dropbox-deepblue_dropbox .ks-dropbox-panel-inner {
    background-color: #fff;
}


